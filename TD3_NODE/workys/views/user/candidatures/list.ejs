<body>
    <h1>
        <%= title %>
    </h1>
    <% if(candidatures.length > 0) { %>
        <table border>
            <thead>
                <tr>
                    <th>SIREN de l'entreprise</th>
                    <th>Nom de l'entreprise</th>
                    <th>Date de cr√©ation</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                <% candidatures.forEach((candidature)=> { %>
                    <tr> 
                        <td> 
                            <%= candidature.siren %> 
                        </td>
                        <td> 
                            <%= candidature.name %> 
                        </td>
                        <td> 
                            <%= candidature.date %> 
                        </td>
                        <td>
                            <button onclick="window.location.href='/user/<%= user.user_id %>/candidatures/<%= candidature.id %>'">
                                Modifier la candidature
                            </button>
                        </td>
                        <td>
                            <button onclick="window.location.href='/user/<%= user.user_id %>/candidatures/<%= candidature.id %>/delete'">
                                Supprimer la candidature
                            </button>
                        </td>
                    </tr>
                <% }) %>
            </tbody>
        <% } else { %>
            Pas de candidature :(
        <% } %>
</body>