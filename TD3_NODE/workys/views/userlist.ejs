<body>
    <h1>
        <%= title %>
    </h1>
    <table border>
        <thead>
            <tr>
                <th>Id</th>
                <th>Firstname</th>
                <th>Lastname</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach((user)=> { %>
                <tr> 
                    <td> 
                        <%= user.id %> 
                    </td>
                    <td> 
                        <%= user.name %> 
                    </td>
                    <td> 
                        <%= user.surname %> 
                    </td> 
                    <td> 
                        <%= user.email %> 
                    </td>
                </tr>
            <% }) %>
        </tbody>
</body>